{
  "name": "pipe.ts",
  "version": "0.1.0",
  "description": "A function composition utility with extra features written in TypeScript",
  "main": "./dist/pipe.js",
  "scripts": {
    "build": "npx rimraf ./dist/*; npx gulp build",
    "format": "prettier --write '*.ts'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BennyHoward/pipe.ts.git"
  },
  "keywords": [
    "function composition",
    "pipe.ts",
    "typescript"
  ],
  "author": "Benny Howard",
  "license": "SEE LICENSE IN ./LICENSE",
  "bugs": {
    "url": "https://github.com/BennyHoward/pipe.ts/issues"
  },
  "homepage": "https://github.com/BennyHoward/pipe.ts",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/sinon": "^7.5.2",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.0",
    "through2": "^3.0.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".(spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "dependencies": {}
}
